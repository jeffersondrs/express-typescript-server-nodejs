{
  "version": 2,

  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "DATABASE_URL": "file:./dev.db"
  }
}